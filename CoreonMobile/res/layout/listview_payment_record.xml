            android:paddingLeft="10dp"
            android:gravity="left|center_vertical"
            android:scaleType="fitCenter"
            android:text="Plan Content"
            android:textColor="#7f7f7f"
            android:textSize="17sp"
            android:textStyle="bold" />
    </LinearLayout>
    
    <LinearLayout
